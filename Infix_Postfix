#include <iostream>
#include <stack>
#include <string>

using namespace std;

int precedence(char c){
    if (c == '/' || c == '*') return 2;
    else if (c =='+' || c == '-') return 1;
    else return 0;
}

bool IsOP(char c){
    return isdigit(c);
}

string infixToPostfix(string s){
    stack<char> st;
    string postFix;
    
    for(size_t i = 0; i < s.length(); i++){
        if(IsOP(s[i])){
            postFix += s[i];
        }
        else if(s[i] == '('){
            st.push('(');
        }
        else if(s[i] == ')'){
            while(!st.empty() && st.top() != '('){
                postFix += st.top();
                st.pop();
            }
            if(!st.empty()) st.pop();
        }else{
            while(!st.empty() && precedence(s[i]) <= precedence(st.top())){
                postFix += st.top();
                st.pop();
            }
            st.push(s[i]);
        }
    }// for
    while(!st.empty()){
        postFix += st.top();
        st.pop();
    }
}// function


int main(){

    string input;
    getline(cin,input);
    
    

return 0;
}
